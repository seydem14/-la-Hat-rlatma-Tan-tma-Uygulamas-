<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".ilacResim">

    <ImageView
        android:id="@+id/imgEtamButol"
        android:layout_width="191dp"
        android:layout_height="184dp"
        android:layout_below="@id/imgEtamButol"

        app:srcCompat="@drawable/etambutol"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="6dp" />

    <ImageView
        android:id="@+id/imgİzoniyazid"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_toRightOf="@id/imgEtamButol"
        app:srcCompat="@drawable/izoniyazid" />

    <ImageView
        android:id="@+id/imgRifampisin"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_below="@id/imgİzoniyazid"

        app:srcCompat="@drawable/rifampisin" />

    <ImageView
        android:id="@+id/imgStreptomisin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgEtamButol"
        android:layout_toRightOf="@id/imgRifampisin"
        app:srcCompat="@drawable/streptomisin" />

    <ImageView
        android:id="@+id/imgPrznamid"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_below="@id/imgRifampisin"
        app:srcCompat="@drawable/pirazinamid" />

    <ImageView
        android:id="@+id/imgMorfo"
        android:layout_toRightOf="@id/imgPrznamid"
        android:layout_below="@id/imgStreptomisin"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        app:srcCompat="@drawable/morfozinamid" />
</RelativeLayout>